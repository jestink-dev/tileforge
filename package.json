{
  "name": "tileforge",
  "version": "1.0.0",
  "description": "Offline map tile server - download, cache, and serve satellite map tiles for offline use",
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "tileforge": "src/cli.js"
  },
  "exports": {
    ".": "./src/index.js",
    "./server": "./src/server/createServer.js",
    "./router": "./src/server/createRouter.js"
  },
  "files": [
    "src/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node src/cli.js serve",
    "test": "node --test"
  },
  "keywords": [
    "tiles",
    "maps",
    "offline",
    "satellite",
    "arcgis",
    "google-maps",
    "openlayers",
    "leaflet",
    "cache",
    "tileserver",
    "geospatial",
    "gis",
    "map-tiles",
    "web-mercator"
  ],
  "author": "Jestin Kuriakose <jestin@fullstackdigital.ca>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jestink-dev/tileforge"
  },
  "bugs": {
    "url": "https://github.com/jestink-dev/tileforge/issues"
  },
  "homepage": "https://github.com/jestink-dev/tileforge#readme",
  "dependencies": {
    "axios": "^1.6.5",
    "better-sqlite3": "^9.2.2",
    "commander": "^12.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2"
  }
}
